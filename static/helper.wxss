/* 边框 */
.bdb {
  border-bottom: 1rpx solid #eeeeee;
}
.bdb.no-last:last-child {
  border-bottom: none !important;
}
.bdb.dark {
  border-color: #dddddd;
}
.bdt {
  border-top: 1rpx solid #eeeeee;
}
.bdt.no-last:last-child {
  border-top: none !important;
}
.bdt.dark {
  border-color: #dddddd;
}
.bd {
