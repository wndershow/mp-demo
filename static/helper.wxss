/* 阴影 */
.bs {
  box-shadow: 0 0 16rpx 0 rgba(0, 0, 0, 0.08);
}
.bs-l {
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.15);
}
.bs-xl {
  box-shadow: 0 0 40rpx rgba(0, 0, 0, 0.2);
}

/* 定位 */
.fix-b {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 10;
  box-sizing: border-box;
  background-color: white;
  box-shadow: 0 -1rpx 10rpx rgba(0, 0, 0, 0.1);
}
.fix-t {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 1;
  box-sizing: border-box;
  background-color: white;
}

.psr {
  position: relative;
}
.psr-t {
  position: relative;
  top: -3rpx;
}

.block {
  display: block !important;
}

.inline {
  display: inline-block;
  vertical-align: middle;
}

/* 垂直居中 */
.weui-flex.v-mdl {
  align-items: center;
  justify-content: center;
}

.hv-mdl {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

/* 圆角 */
.br {
  border-radius: 10rpx !important;
}
.br-l {
  border-radius: 20rpx !important;
}
.br-xl {
  border-radius: 32rpx !important;
}

/* 宽度 */
.w-50 {
  width: 50%;
}

/* 旋转 */
.r-90 {
  transform: rotate(90deg) !important;
}
